{
  "extends": ["airbnb", "prettier"],
  "parser": "babel-eslint",
  "plugins": ["prettier"],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".jsx", ".js"]
      }
    ],
    "import/no-extraneous-dependencies": 0,
    "import/prefer-default-export": 0,
    "react/require-default-props": 0,
    "linebreak-style": ["error", "unix"],
    "react/button-has-type": 0,
    "react/prop-types": 0,
    "no-plusplus": 0,
    "no-undef": 0
  }
}
